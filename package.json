        "prop-types": "^15.7.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-process-string": "^1.2.0",
        "react-redux": "^8.1.1",
        "react-router-dom": "^6.13.0",
        "redux": "^4.2.1",
        "redux-saga": "^1.2.3",
