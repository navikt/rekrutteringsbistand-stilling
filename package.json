    "repository": "https://github.com/navikt/rekrutteringsbistand-stilling",
    "scripts": {
        "start": "craco start",
        "start:export": "BROWSER=none REACT_APP_EXPORT=true craco start",
        "start:mock": "REACT_APP_MOCK=true craco start",
        "start:mock:export": "BROWSER=none REACT_APP_EXPORT=true REACT_APP_MOCK=true craco start",
        "build": "REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD) craco build",
        "sentry-release": "(export SENTRY_RELEASE=$(git rev-parse --short HEAD); node scripts/sentry-release)",
        "test": "craco test",
        "start-express": "node server/server.js",
        "docker-build": "npm run build && docker build . -t rekrutteringsbistand-stilling",
        "docker-start": "docker run --name rekrutteringsbistand-stilling -p 8080:8080 -d rekrutteringsbistand-stilling",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm rekrutteringsbistand-stilling",
        "docker-stop": "docker stop rekrutteringsbistand-stilling",
        "protect": "npm run snyk-protect",
        "test:snyk": "snyk test"
    },
