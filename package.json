        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "pretty-quick": "^3.1.2",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
